export CONTAINER_NAME=machine-learning
export IMAGE_NAME=tkemmerling/machine-learning-tf
export WORKDDIR=/root
export AWS_PROFILE=default

docker run --rm -t --name=${CONTAINER_NAME} \
           -p 8888:8888 \
           -e AWS_PROFILE=${AWS_PROFILE} \
           -v ~/.aws:${WORKDDIR}/.aws:ro \
           -v $PWD:/opt/app \
           ${IMAGE_NAME} 

